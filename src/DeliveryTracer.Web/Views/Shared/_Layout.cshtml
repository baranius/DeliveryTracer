<!DOCTYPE html>
<html ng-app="tracerApp">
<head>
    <title>Delivery Tracer</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
    <link rel="stylesheet" href="@ViewBag.Host/Content/app.css">

    <script src="http://code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.16/angular.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.16/angular-route.js"></script>
    <script src="@ViewBag.Host/Content/app.js" type="text/javascript"></script>
    <script src="@ViewBag.Host/Content/ajax.js" type="text/javascript"></script>
</head>
<body>
    <div class="navbar navbar-inverse" ng-controller="topMenuController as tpc">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" href="/#/">{{tpc.pageTitle}}</a>
            </div>

            <ul class="nav navbar-nav" style="float:right">
                <li><a href="#/">Logs</a></li>
                <li><a href="#pipelines">Pipelines</a></li>
                
            </ul>
            </div>
    </div>
    
    @RenderBody()
    

